[sub_resource type="GDScript" id="GDScript_s5ose"]
resource_name = "Toast"
script/source = "extends Label

var vertical_slide: float = 1.01
var toast_time: float = 20

func _ready() -> void:
	var tween = get_tree().create_tween()
	tween.tween_property(self,\"global_position\",global_position * (Vector2.UP * vertical_slide),toast_time)
	await tween.finished
	queue_free()
"

